{
    "name": "zenaton/examples-php",
    "license": "Apache-2.0",
    "require": {
        "zenaton/zenaton-php": "@dev"
    },
    "repositories": {
        "dev-package": {
            "type": "path",
            "url": "../zenaton-php",
            "options": {
              "symlink": true
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
